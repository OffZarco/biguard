<form method="post" action="{{ path('app_admin_entree_delete', {'id': entree.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ entree.id) }}">
    <button class="btn"><i class="fa-solid fa-trash"></i></button>
</form>
